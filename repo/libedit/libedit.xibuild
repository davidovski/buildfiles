#!/bin/sh

MAKEDEPS="make automake autoconf"
DEPS="musl ncurses"

PKG_VER=20210910-3.1
SOURCE=https://www.thrysoee.dk/editline/libedit-20210910-3.1.tar.gz
DESC="BSD line editing library"

prepare () {
    autoreconf -fi
}

build () {
    ./configure --prefix=/usr 
    make
}

package () {
    make DESTDIR=$PKG_DEST install
}


