#!/bin/sh

NAME="ucpp"
DESC="C preprocessor which is embeddable, quick, light and C99 compliant"

MAKEDEPS=""

PKG_VER=1.3.5
SOURCE="https://gitlab.com/scarabeusiv/ucpp"

prepare () {
    autoreconf -vif
}

build() {
	./configure \
		--prefix=/usr \
		--disable-static
	make
}

package() {
	make DESTDIR="$PKG_DEST" install
}

