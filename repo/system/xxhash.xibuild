#!/bin/bash

MAKEDEPS=(make )
DEPS=(sh)

PKG_VER=0.8.1
SOURCE=https://github.com/Cyan4973/xxHash/archive/refs/tags/v$PKG_VER.tar.gz
DESC="xxHash is an Extremely fast Hash algorithm"


build () {
    make PREFIX=/usr -C xxHash
}

package () {
    make PREFIX=/usr DESTDIR=$PKG_DEST -C xxHash install
}

