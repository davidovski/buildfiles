#!/bin/bash

MAKEDEPS=(make )
DEPS=(glibc)

PKG_VER=1.16
SOURCE=https://ftp.gnu.org/pub/gnu/libiconv/libiconv-$PKG_VER.tar.gz

DESC="GNU charset conversion library"

build () {
    ./configure --prefix=/usr --disable-static &&
    make
}

package () {
    make prefix=/usr lib=lib DESTDIR=$PKG_DEST install
}

