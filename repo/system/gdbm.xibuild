#!/bin/bash

DEPS=(glibc sh readline)

PKG_VER=1.21
SOURCE=https://ftp.gnu.org/gnu/gdbm/gdbm-$PKG_VER.tar.gz
DESC="GNU database library"

build () {
    ./configure --prefix=/usr --disable-static --enable-libgdm-compat
    make
}

check () {
    make -k check
}

package () {
    make DESTDIR=$PKG_DEST install 
}
