#!/bin/sh

MAKEDEPS="make sort"
DEPS="musl sh"

PKG_VER=0.8.8
SOURCE=http://landley.net/toybox/downloads/toybox-$PKG_VER.tar.gz
DESC="Utility programs for creating patch files"

build () {
    make defconfig
    make diff
    make cmp
}

package () {
    install -Dm755 diff $PKG_DEST/usr/bin/
    install -Dm755 cmp $PKG_DEST/usr/bin/
}
